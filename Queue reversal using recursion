class Solution
{
    public:
    void reverse(queue<int>&q){
         if(q.empty()){
        return;
        }
       int frnt=q.front();
       q.pop();
       
       reverse(q);
       
       q.push(frnt);
       
       
    }
    queue<int> rev(queue<int> q)
    {
       reverse(q);
       return q;
        // add code here.
    }
};
