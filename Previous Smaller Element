#include<bits/stdc++.h>
vector<int> prevSmallerElement(vector<int> &arr, int n)
{
    stack<int>s;
    
    s.push(-1);
    vector<int>ans(n);
    for(int i=0;i<n;i--){
        int curr=arr[i];
        while(s.top()>=curr){
            s.pop();
        }
       
        ans[i]=s.top();
        s.push(curr);
    }
    return ans;
    // Write your code here.
}
