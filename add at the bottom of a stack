//Iterative Approach
#include <bits/stdc++.h> 
stack<int> pushAtBottom(stack<int>& myStack, int x) 
{
    stack<int>s;
    int n=myStack.size();
    int m=myStack.size();
    for (int i = 0; i < n; i++) {
      s.push(myStack.top()); 
      myStack.pop();
    }

    myStack.push(x);
    
    for(int i=0;i<m;i++){
        myStack.push(s.top());
        s.pop();
    }
    return myStack;
    // Write your code here.
}

